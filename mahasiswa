public class mahasiswa {
       void membaca () {
       System.out.println("HARUS RAJIN MEMBACA...membaca");
   }
   void nyontek () {
           System.out.println("JANGAN MENYONTEK...nyontek");
   }
   void modifikasi () {
       System.out.println("HARUS MEMODIFIKASI MATERI...materi");
   }
   void semua () {
       System.out.println("HARUS RAJIN MEMBACA...membaca");
       System.out.println("JANGAN MENYONTEK...nyontek");
       System.out.println("HARUS MEMODIFIKASI MATERI...materi");
   }
}
