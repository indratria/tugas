public  class  Matematika{
   int hasil;
 
    void pertambahan (int a, int b){
    System.out.println(a+"+"+b+"="+(a+b) );
    }
    void pengurangan (int a, int b){
    System.out.println(a+"-"+b+"="+(a-b) );
    }
    void perkalian (int a, int b){
    System.out.println(a+"*"+b+"="+(a*b) );
    }
    void pembagian (int a, int b){
    System.out.println(a+"/"+b+"="+(a/b) );
    }
    
